 <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="0" Grid.Row="0" Text="Team/Quartal" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>

        <TextBlock Grid.Column="1" Grid.Row="0" Text="{Binding DisplayQuarters[0]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="2" Grid.Row="0" Text="{Binding DisplayQuarters[1]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="3" Grid.Row="0" Text="{Binding DisplayQuarters[2]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="4" Grid.Row="0" Text="{Binding DisplayQuarters[3]}" Style="{StaticResource QuarterStyle}"></TextBlock>

        <TextBlock Grid.Column="0" Grid.Row="1" Text="{Binding DisplayTeams[0]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="2" Text="{Binding DisplayTeams[1]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="3" Text="{Binding DisplayTeams[2]}" Style="{StaticResource QuarterStyle}"></TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="4" Text="{Binding DisplayTeams[3]}" Style="{StaticResource QuarterStyle}"></TextBlock>

       

       

    </Grid>