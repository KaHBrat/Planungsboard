<Application x:Class="Planungsboard.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Planungsboard.Presentation"
             xmlns:viewModels="clr-namespace:Planungsboard.Presentation.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <viewModels:ViewModelLocator x:Key="Locator" />
                </ResourceDictionary>
                <ResourceDictionary Source="DesignData/ResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>